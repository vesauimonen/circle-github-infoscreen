{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "react/jsx-key": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/prefer-es6-class": 1,
        "react/react-in-jsx-scope": 1,
        "react/wrap-multilines": 1
    },
    "env": {
        "amd": true,
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "plugins": [
        "react"
    ]
}
